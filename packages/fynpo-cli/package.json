{
  "name": "fynpo-cli",
  "version": "1.0.2",
  "description": "npm install globally to provide the fynpo command",
  "preferGlobal": true,
  "main": "index.js",
  "bin": {
    "fynpo": "dist/fynpo.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "xrun --serial [build, xarc/check]",
    "docs": "xrun xarc/docs",
    "ci:check": "xrun xarc/check",
    "prepack": "publish-util-prepack",
    "postpack": "publish-util-postpack"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "fyn",
    "lerna",
    "repo",
    "mono",
    "monorepo"
  ],
  "author": "Walmart GTP.js Team",
  "contributors": [
    "Joel Chen",
    "Divya Vannilaparambath"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/fynpo.git"
  },
  "dependencies": {
    "require-at": "^1.0.6",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^14.14.16",
    "@xarc/module-dev": "^3.2.1",
    "prettier": "^2.2.1",
    "publish-util": "^1.0.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.13",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "printWidth": 100
  },
  "@xarc/module-dev": {
    "features": [
      "prettier",
      "typedoc",
      "typescript"
    ]
  }
}
